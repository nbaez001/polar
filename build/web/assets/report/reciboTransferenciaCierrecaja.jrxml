	<queryString>
		<![CDATA[SELECT
pe.dni as dni,
c.id as idCajero
FROM
     transferencia tr
INNER JOIN cajero c ON c.id= tr.idCajero1
INNER JOIN usuario u ON u.id = tr.idUsuario
INNER JOIN empleado em ON em.id = u.idEmpleado
INNER JOIN persona pe ON pe.id = em.idPersona
WHERE tr.id = $P{idTransferencia}]]>
